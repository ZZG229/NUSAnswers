<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".UserInformation">

    <TextView
        android:id="@+id/textView19"
        android:layout_width="344dp"
        android:layout_height="5dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="139dp"
        android:background="#F2994A"
        android:text="TextView" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="347dp"
            android:layout_height="29dp"
            android:layout_centerInParent="true"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/textView4"
            android:layout_marginBottom="198dp"
            android:text="@string/current_enrolled_modules"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="#000000"
            android:textSize="15sp"
            android:textStyle="bold"
            tools:typeface="serif" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="230dp"
            android:background="#8D8D8D"
            android:text="TextView" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="61dp"
        android:background="#8D8D8D"
        android:text="TextView" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="141dp"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="22dp"
            app:srcCompat="@drawable/logo" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/textView5"
            android:layout_marginTop="228dp"
            android:text="@string/completed_modules"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="#000000"
            android:textSize="15sp"
            android:textStyle="bold"
            tools:typeface="serif" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="346dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="137dp"
            android:text="@string/which_programme_are_you_enrolled_in"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="#000000"
            android:textSize="15sp"
            android:textStyle="bold"
            tools:typeface="serif" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="344dp"
            android:layout_height="5dp"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="252dp"
            android:background="#F2994A"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="217dp"
            android:background="#8D8D8D"
            android:text="TextView" />

        <Button
            android:id="@+id/button121"
            style="@android:style/Widget.Button"
            android:layout_width="67dp"
            android:layout_height="29dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="12dp"
            android:layout_marginEnd="11dp"
            android:background="#F2A968"
            android:text="@string/next" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="347dp"
        android:layout_height="29dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="153dp"
        android:text="TextView" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="347dp"
            android:layout_height="29dp"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/textView15"
            android:layout_marginTop="165dp"
            android:text="TextView" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="88dp"
        android:layout_height="32dp"
        android:layout_alignStart="@+id/textView19"
        android:layout_alignTop="@+id/button2"
        android:text="@string/skip_for_now"
        android:textColor="#000000" />

</RelativeLayout>
